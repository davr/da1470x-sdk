	<cs_commands>
	
		<cs_command>
			<type>START</type>
			<name>Start</name>
			<description>Indicates that a valid CS is in place</description>
			<identifier>
				<value>A5A5A5A5</value>
				<comparator>equals</comparator>
			</identifier>
			<size>1</size> <!--in words-->
		</cs_command>
		
		<cs_command>
			<type>REGS</type>
			<name>Register Configuration</name>
			<description>_registerName_#_registerName_ value</description>
			<identifier> <!--Dynamic identifier, a valid register address-->
				<value></value>
				<comparator></comparator>
			</identifier>
			<size>2</size> <!--in words-->
		</cs_command>
		
		<cs_command>
			<type>TRIM_CALIB</type>  <!--SDK section cmd is described as Trim/Calib values for other devices. Use TRIM_CALIB type for backwards compatibility purposes-->
			<name>Trim Value</name>
			<description>_groupName_, _num_ values</description>
			<identifier>
				<value>E900</value>
				<comparator>startsWith</comparator>
			</identifier>
			<size>0</size> <!--variable size, set to 0-->
		</cs_command>
		
		<cs_command>
			<type>BOOTER</type>
			<name>Booter Value</name>
			<description>Address of the product header in flash is _PHaddress_</description>
			<identifier>
				<value>E6000000</value>
				<comparator>equals</comparator>
			</identifier>
			<size>2</size> <!--in words-->
		</cs_command>
		
		<cs_command>
			<type>DEVEL_MODE</type>
			<name>Development Mode disable</name>
			<description>Disable development mode</description>
			<identifier>
				<value>E7000000</value>
				<comparator>equals</comparator>
			</identifier>
			<size>1</size> <!--in words-->
		</cs_command>
		
		<cs_command>
			<type>UART_STX</type>
			<name>Uart STX timeout</name>
			<description>Timeout is _stxTimeout_. Baudrate is _baudrate_</description>
			<identifier>
				<value>E8</value>
				<comparator>startsWith</comparator>
			</identifier>
			<size>1</size> <!--in words-->
			<baudrates default="115200">
				<baudrate>
					<value>1</value>
					<bd>4800</bd>
				</baudrate>
				<baudrate>
					<value>2</value>
					<bd>9600</bd>
				</baudrate>
				<baudrate>
					<value>3</value>
					<bd>14400</bd>
				</baudrate>
				<baudrate>
					<value>4</value>
					<bd>19200</bd>
				</baudrate>
				<baudrate>
					<value>5</value>
					<bd>28800</bd>
				</baudrate>
				<baudrate>
					<value>6</value>
					<bd>38400</bd>
				</baudrate>
				<baudrate>
					<value>7</value>
					<bd>57600</bd>
				</baudrate>
				<baudrate>
					<value>8</value>
					<bd>115200</bd>
				</baudrate>
				<baudrate>
					<value>9</value>
					<bd>230400</bd>
				</baudrate>
				<baudrate>
					<value>10</value>
					<bd>500000</bd>
				</baudrate>
				<baudrate>
					<value>11</value>
					<bd>1000000</bd>
				</baudrate>
			</baudrates>
		</cs_command>
		
		<cs_command>
			<type>XTAL_SETTLE</type>
			<name>Xtal Settling</name>
			<description>. XTAL32M settling time is _xtalSettling_.</description>
			<identifier>
				<value>EA</value>
				<comparator>startsWith</comparator>
			</identifier>
			<xtal_options>
				<option>
					<value>1</value>
					<text>Ignore XTAL32M settling time and just continue</text>
				</option>
				<option>
					<value>2</value>
					<text>Use XTAL32M as the default booter clock instead of the RCHS</text>
				</option>
			</xtal_options>
			<size>1</size> <!--in words-->
		</cs_command>
		
		<cs_command>
			<type>MIN_FW_VERSION</type>
			<name>Minimunm FW version</name>
			<description>Minimum FW version</description>
			<identifier>
				<value>EB000000</value>
				<comparator>equals</comparator>
			</identifier>
			<size>2</size> <!--in words-->
		</cs_command>
		
		<cs_command>
			<type>STOP</type>
			<name>Stop</name>
			<description>Indicates that the configuration script has reached the end and execution should be terminated</description>
			<identifier>
				<value>00000000</value>
				<comparator>equals</comparator>
			</identifier>
			<size>1</size> <!--in words-->
		</cs_command>
	</cs_commands>
